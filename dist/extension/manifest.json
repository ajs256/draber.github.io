{
  "manifest_version": 2,
  "name": "Spelling Bee Assistant",
  "description": "Spelling Bee Assistant is a JavaScript bookmarklet for Spelling Bee, the New York Timesâ€™ popular word puzzle",
  "homepage_url": "https://spelling-bee-assistant.app/",
  "version": "2.0.0",
  "content_scripts": [{
    "matches": [
      "https://www.nytimes.com/puzzles/spelling-bee"
    ],
    "js": ["assets/content.js"]
  }],
  "background": {
    "scripts": [
      "assets/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": ["assets/spelling-bee-assistant.min.js"],
  "permissions": [
    "webNavigation",
    "storage"
  ]
}